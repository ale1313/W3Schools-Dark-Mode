{
  "manifest_version": 2,
  "name": "W3Schools Dark Mode",
  "description": "Dark mode for W3 website",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/index.html"
  }
}